<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : stocks_conteos.xml
    Author     : Juan Jose Prieto Dzul
    Description:
        Structure for the conteosinventarios table.
-->
<table>
    <column>
        <name>codalmacen</name>
        <type>character varying(4)</type>
    </column>
    <column>
        <name>descripcion</name>
        <type>character varying(30)</type>
        <null>NO</null>
    </column>
    <column>
        <name>editable</name>
        <type>boolean</type>
    </column>
    <column>
        <name>fechafin</name>
        <type>date</type>
    </column>
    <column>
        <name>fechainicio</name>
        <type>date</type>
        <null>NO</null>
    </column>
    <column>
        <name>horainicio</name>
        <type>time</type>
        <null>NO</null>
    </column>
    <column>
        <name>horafin</name>
        <type>time</type>
    </column>
    <column>
        <name>idconteo</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>idempresa</name>
        <type>integer</type>
    </column>
    <column>
        <name>observaciones</name>
        <type>text</type>
    </column>
    <column>
        <name>nick</name>
        <type>character varying(50)</type>
    </column>
    <constraint>
        <name>stocks_conteos_pkey</name>
        <type>PRIMARY KEY (idconteo)</type>
    </constraint>
    <constraint>
        <name>ca_stocks_conteos_almacenes</name>
        <type>FOREIGN KEY (codalmacen) REFERENCES almacenes (codalmacen) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_stocks_conteos_empresas</name>
        <type>FOREIGN KEY (idempresa) REFERENCES empresas (idempresa) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_stocks_conteos_users</name>
        <type>FOREIGN KEY (nick) REFERENCES users (nick) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
</table>